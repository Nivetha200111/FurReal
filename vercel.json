{
  "version": 2,
  "builds": [
    {
      "src": "api/health.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/analyze/post.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/api/analyze/post",
      "dest": "/api/analyze/post.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}