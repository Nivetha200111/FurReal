{
  "version": 2,
  "functions": {
    "api/health.js": {
      "runtime": "nodejs18.x"
    },
    "api/analyze/post.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/api/analyze/post",
      "dest": "/api/analyze/post.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}